<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NegoList</servlet-name>
        <jsp-file>/NegoList.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Negotiation</servlet-name>
        <jsp-file>/Negotiation.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>RoleServlet</servlet-name>
        <servlet-class>Ctrl.RoleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminList</servlet-name>
        <jsp-file>/AdminList.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>AdminManage</servlet-name>
        <jsp-file>/AdminManage.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>AdminNew</servlet-name>
        <jsp-file>/AdminNew.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>AdminModify</servlet-name>
        <jsp-file>/AdminModify.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Report</servlet-name>
        <jsp-file>/Reporting.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Reporting</servlet-name>
        <jsp-file>/Reporting.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>acceptFile</servlet-name>
        <jsp-file>/acceptFile.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>NegoList</servlet-name>
        <url-pattern>/NegoList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Negotiation</servlet-name>
        <url-pattern>/Negotiation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoleServlet</servlet-name>
        <url-pattern>/RoleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminList</servlet-name>
        <url-pattern>/AdminList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminManage</servlet-name>
        <url-pattern>/AdminManage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminNew</servlet-name>
        <url-pattern>/AdminNew</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminModify</servlet-name>
        <url-pattern>/AdminModify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Report</servlet-name>
        <url-pattern>/Report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reporting</servlet-name>
        <url-pattern>/Reporting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>acceptFile</servlet-name>
        <url-pattern>/acceptFile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
